@{
    ViewBag.Title = "SendMessage";
}




<body>
    <script src="/TrackingSystem/Scripts/PrivateChat.js"></script>
<link href="/TrackingSystem/Content/StyleSheet1.css" rel="stylesheet" />
    <div class="container-fluid">
        <ul class="nav nav-tabs">
                <li role="presentation"><a href="/TrackingSystem/Home/AddUser">Add User</a></li>
            <li role="presentation"><a href="/TrackingSystem/Home/ShowDrivers">Show Drivers</a></li>
            <li role="presentation" ><a href="/TrackingSystem/Home/ShowHistory">History</a></li>
            <li role="presentation" class="active"><a href="/TrackingSystem/Home/SendMessage">Send Message</a></li>
        </ul>
        <div id="divLogin" class="login">
            <input id="txtNickName" type="text" class="textBox" value="Admin" readonly="true" />
            <div id="divButton">
                <input id="btnStartChat" type="button" class="submitButton" value="Start Chat" />
            </div>
        </div>

        <div id="divChat" class="chatRoom">
            <h4>Send Messages here...</h4>

            <div class="title">
                Welcome [<span id='spanUser'></span>]

            </div>
            <div class="content">
                <div id="divChatWindow" class="chatWindow">
                </div>
                <div id="divusers" class="users">
                </div>
            </div>
            <div class="messageBar">
                <input class="textbox" type="text" id="txtMessage" />
                <input id="btnSendMsg" type="button" value="Send" class="submitButton" />
            </div>
        </div>

        <input id="hdId" type="hidden" />
        <input id="hdUserName" type="hidden" />
     <div id="divContainer"></div>
        </div>    
      <script src="/TrackingSystem/Scripts/jquery-1.8.2.min.js"></script>

    <script src="/TrackingSystem/Scripts/ui/jquery.ui.core.js"></script>
    <script src="/TrackingSystem/Scripts/ui/jquery.ui.widget.js"></script>
    <script src="/TrackingSystem/Scripts/ui/jquery.ui.mouse.js"></script>
    <script src="/TrackingSystem/Scripts/ui/jquery.ui.draggable.js"></script>
    <script src="/TrackingSystem/Scripts/ui/jquery.ui.resizable.js"></script>
    <!--Reference the SignalR library. -->
    <script src="/TrackingSystem/Scripts/jquery.signalR-1.0.0.js"></script>
        <!--Script references. -->
    <!--Reference the jQuery library. -->
     <script src="/TrackingSystem/Scripts/jquery-1.6.4.min.js" "></script>
    <!--Reference the SignalR library. -->
    <script src="/TrackingSystem/Scripts/jquery.signalR-2.0.0.js"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="/TrackingSystem/signalr/hubs"></script>

</body>
 