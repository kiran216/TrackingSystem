@{
    ViewBag.Title = "User";
}

<h2>User</h2>
<meta charset="utf-8">
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/gmaps.js/0.4.21/gmaps.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/gmaps.js/0.4.21/gmaps.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/gmaps.js/0.4.21/gmaps.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/gmaps.js/0.4.21/gmaps.min.js.map"></script>


<script src="/TrackingSystem/Scripts/SaveUserLocation.js"></script>
<link rel="stylesheet" href="http://twitter.github.com/bootstrap/1.3.0/bootstrap.min.css" />
<link href="/TrackingSystem/Content/StyleSheet1.css" rel="stylesheet" />

<link href="/TrackingSystem/Content/StyleSheet1.css" rel="stylesheet" />
<div class="container" id="div1">
    <div id="divLogin" class="login">
        <div>
            Enter your Name:<br />
            <input id="txtNickName" type="text" class="textBox" placeholder="please enter name here" />
        </div>
        <div id="divButton">
            <input id="btnStartChat" type="button" class="submitButton" value="Connect" />
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div id="map_canvas" style="width: 100%; height: 460px"></div>
        </div>
        <div class="col-md-6">

            <div id="divChat" class="chatRoom">
                <div class="title">
                    <b>Chat Box</b>
                </div>
                <div class="content">
                    <div id="divChatWindow" class="chatWindow">
                    </div>
                    <b>Users</b>
                    <div id="divusers" class="users">
                    </div>
                </div>
                <div class="messageBar">
                    <input class="textbox" type="text" id="txtMessage" />
                    <input id="btnSendMsg" type="button" value="Send" class="submitButton" />
                </div>
                <input id="hdId" type="hidden" />
                <input id="hdUserName" type="hidden" />
            </div>


        </div>
    </div>
    <div class="row">
        <div id="divContainer">
        </div>
    </div>
    <!--Reference the SignalR library. -->
    <script src="/TrackingSystem/Scripts/jquery.signalR-1.0.0.js"></script>
    <!--Script references. -->
    <!--Reference the jQuery library. -->

    <!--Reference the SignalR library. -->
    <script src="/TrackingSystem/Scripts/jquery.signalR-2.0.0.js"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="/TrackingSystem/signalr/hubs"></script>
</div>
